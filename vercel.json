{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "NODE_ENV": "production"
  },
  "functions": {
    "api/analyze.js": {
      "maxDuration": 300,
      "memory": 3008
    }
  },
  "regions": ["sin1"],
  "framework": null
}